	<xs:element name="IOGPEC05_RET">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="IOGPEC05_SQB">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:string" name="QB_DEPTO"/>
							<xs:element type="xs:string" name="QB_DESCRIC"/>
							<xs:element type="xs:string" name="RD4_CHAVE"/>
							<xs:element type="xs:string" name="RD4_TREE"/>
