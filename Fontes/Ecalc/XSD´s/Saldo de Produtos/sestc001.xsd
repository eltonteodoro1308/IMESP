<xs:pattern value="1\.01"/>
</xs:restriction>
</xs:simpleType>
<xs:simpleType name="MPdeleted">
<xs:annotation>
<xs:documentation>Status da linha da estrutura do tipo GRID :
1 - deletada
0 - ativa
</xs:documentation>
</xs:annotation>
